{
  "name": "opui-css",
  "version": "3.1.1",
  "private": false,
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "style": "dist/ui.css",
  "exports": {
    ".": "./dist/ui.css",
    "./dist/*": "./dist/*",
    "./src/*": "./src/*"
  },
  "scripts": {
    "build:themes": "node scripts/build-themes.js",
    "build": "pnpm run clean && pnpm run build:themes && postcss src/open-props.css -o dist/op.css",
    "clean": "pnpm exec shx rm -rf dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@docsearch/css": "^4.3.2",
    "@docsearch/js": "^4.3.2",
    "@shikijs/core": "^3.10.0",
    "@shikijs/transformers": "^3.10.0",
    "@types/markdown-it": "^14.1.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/devtools-api": "^8.0.3",
    "@vue/shared": "^3.5.18",
    "@vueuse/core": "^14.0.0",
    "@vueuse/integrations": "^14.0.0",
    "baseline-status": "^1.0.11",
    "focus-trap": "^7.6.5",
    "mark.js": "^8.11.1",
    "minisearch": "^7.1.2",
    "opui-blocks": "^0.2.0",
    "shiki": "^3.10.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "date-fns": "^4.1.0",
    "gray-matter": "^4.0.3",
    "nodemon": "^3.1.10",
    "open-props": "2.0.0-beta.5",
    "postcss-cli": "^11.0.1",
    "postcss-custom-media": "^11.0.6",
    "postcss-import": "^16.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitepress": "^1.6.4",
    "vue-tsc": "^3.1.4"
  }
}
